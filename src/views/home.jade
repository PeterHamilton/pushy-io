extends layout

block script
  script(src='/javascripts/main.coffee', type='text/coffeescript')

block content
  #image-container.view-pane.centered
            img(src='')

  .container-fluid
    .row-fluid
      #main-content
        #loading.loading
        #text.view-pane
          table(width="100%", height="800px")
            tr
              td(valign="center", align="center")

        #website-iframe.view-pane(style="display:none")

        #dashboard.view-pane(style="display:none")
          .span8
            h1 Notifications
            #notifications

          .span4
            h1 Twitter Feed
            #twitter-widget
              a.twitter-timeline(href="https://twitter.com/search?q=%23IChack", "data-widget-id"="300189295266893824", height="850")
              script(type='text/javascript')
                !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

